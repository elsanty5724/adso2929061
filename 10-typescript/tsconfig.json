{
  // Visit https://aka.ms/tsconfig to read more about this file
  "compilerOptions": {
    "module": "ES6",
    "target": "ES6",
    "rootDir": "./src",
    "outDir": "./dist",
    "strict": true,

    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "experimentalDecorators": true

  },
  "include":["src/**/*"],
  "exclude": ["node_modules"]
}